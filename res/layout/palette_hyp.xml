<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/palette_hyp"
    android:tag="@string/tag_plt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:stretchColumns="@string/stretchPaletteCols"
     >
    
	<TableRow>
		<awqatty.b.OpButtons.OperationButton
			android:id="@+id/buttonHypSin"
			android:tag="@string/tag_op"
			android:text="@string/hypsine"
			android:textSize="@dimen/button_textSize_M"
			style="?android:attr/borderlessButtonStyle"
			 />
		<awqatty.b.OpButtons.OperationButton
			android:id="@+id/buttonHypCos"
			android:tag="@string/tag_op"
			android:text="@string/hypcosine"
			android:textSize="@dimen/button_textSize_M"
			style="?android:attr/borderlessButtonStyle"
			 />
		<awqatty.b.OpButtons.OperationButton
			android:id="@+id/buttonHypTan"
			android:tag="@string/tag_op"
			android:text="@string/hyptangent"
			android:textSize="@dimen/button_textSize_M"
			style="?android:attr/borderlessButtonStyle"
			 />
	</TableRow>
	
	<TableRow>
		<awqatty.b.OpButtons.OperationButton
			android:id="@+id/buttonAHypSin"
			android:tag="@string/tag_op"
			android:text="@string/arhypsine"
			android:textSize="@dimen/button_textSize_S"
			style="?android:attr/borderlessButtonStyle"
			 />
		<awqatty.b.OpButtons.OperationButton
			android:id="@+id/buttonAHypCos"
			android:tag="@string/tag_op"
			android:text="@string/arhypcosine"
			android:textSize="@dimen/button_textSize_S"
			style="?android:attr/borderlessButtonStyle"
			 />
		<awqatty.b.OpButtons.OperationButton
			android:id="@+id/buttonAHypTan"
			android:tag="@string/tag_op"
			android:text="@string/arhyptangent"
			android:textSize="@dimen/button_textSize_S"
			style="?android:attr/borderlessButtonStyle"
			 />
			
	</TableRow>
</TableLayout>
