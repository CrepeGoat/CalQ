<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="awqatty.b.calq"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />
    <supports-screens 
        android:anyDensity="true" 
        android:smallScreens="false" 
        android:largeScreens="true" 
        android:normalScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="awqatty.b.calq.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="awqatty.b.calq.SettingsActivity"
            android:label="@string/activityLabel_settings"
            android:parentActivityName="awqatty.b.calq.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="awqatty.b.calq.MainActivity" />            
        </activity>
        
    </application>

</manifest>
